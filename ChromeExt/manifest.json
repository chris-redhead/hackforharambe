{
  "manifest_version": 2,

  "name": "Image encrypt/decrypt",
  "description": "blah blah blah",
  "version": "0.0.0.1",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "permissions": [
    "activeTab",
    "contextMenus"
  ],
  
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },

  
  "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
  }

}